    "@mui/material": "^5.18.0",
    "@mui/x-date-pickers": "^8.11.1",
    "@tanstack/react-query": "^5.85.9",
    "axios": "^1.11.0",
    "d3-scale": "^4.0.2",
    "d3-time": "^3.1.0",
